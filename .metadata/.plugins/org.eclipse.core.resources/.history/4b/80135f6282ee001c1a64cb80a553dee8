package poo;

public class Coche {
	
	private int ruedas;
	
	private int largo;
	
	private int ancho;
	
	private int motor;
	
	private int peso_plataforma;
	
	String color;
	
	int peso_total;
	
	boolean asientos_cuero, climatizador;
	
	
	public Coche() {
		
		ruedas = 4;
		
		largo = 2000;
		
		motor = 1600;
		
		peso = 500;
		
	}
	
	public int getRuedas() {
		return ruedas;
	}
	
	public void setRuedas(int ruedas) {
		this.ruedas = ruedas;
	}

}
